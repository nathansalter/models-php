{
  "name": "streamline/openactive-models",
  "description": "Fork of the openactive/models library. See https://github.com/openactive/models-php for original library",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "OpenActive\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenActive\\Models\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^5.7"
  },
  "scripts": {
    "fix": "./vendor/bin/php-cs-fixer fix",
    "test": "./vendor/bin/phpunit"
  }
}
